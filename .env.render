# Environment Variables for Render Deployment
# Copy these to Render's Environment Variables section

# Next.js (Production)
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1

# Blockchain & Web3
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here
NEXT_PUBLIC_ESCROW_CONTRACT_ADDRESS=0xc7cd93d40ae3e3d913706e28a3cfc0fc67477fb4
NEXT_PUBLIC_USDC_CONTRACT_ADDRESS=0x8B0180f2101c8260d49339abfEe87927412494B4

# Supabase (Database)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# IPFS / Pinata (File Storage)
PINATA_JWT=your_pinata_jwt_token_here

# AI / Gemini
GEMINI_API_KEY=your_gemini_api_key_here
